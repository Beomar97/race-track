<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="vbox" prefHeight="200.0" prefWidth="100.0"
      stylesheets="@../css/bootstrap.css" xmlns="http://javafx.com/javafx">
   <children>
      <HBox fx:id="hboxImageView" alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <ImageView fitHeight="526.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../logo/RaceTrack_Logo.png"/>
               </image>
            </ImageView>
         </children>
      </HBox>
      <BorderPane prefHeight="200.0" prefWidth="200.0">
         <bottom>
            <GridPane alignment="CENTER" BorderPane.alignment="CENTER">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                  <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
               </rowConstraints>
               <children>
                  <Button fx:id="btnMainMenu" alignment="CENTER" mnemonicParsing="false" text="Main Menu"
                          GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
                  <ImageView fitHeight="35.0" fitWidth="149.0" pickOnBounds="true" preserveRatio="true"
                             GridPane.columnIndex="2">
                     <image>
                        <Image url="@../logo/PathFinder_Logo.png"/>
                     </image>
                  </ImageView>
               </children>
               <BorderPane.margin>
                  <Insets bottom="10.0"/>
               </BorderPane.margin>
            </GridPane>
         </bottom>
         <center>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="RaceTrack v1.0" textAlignment="CENTER">
                     <font>
                        <Font size="32.0"/>
                     </font>
                  </Text>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Build #23k29lv, built on May 8, 2020"
                        textAlignment="CENTER">
                     <VBox.margin>
                        <Insets bottom="30.0"/>
                     </VBox.margin>
                     <font>
                        <Font size="18.0"/>
                     </font>
                  </Text>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Marco Forster">
                     <font>
                        <Font size="18.0"/>
                     </font>
                  </Text>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="github.com/Beomar97">
                     <VBox.margin>
                        <Insets bottom="10.0"/>
